<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>bfsl.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FetchApi</servlet-name>
  	<display-name>FetchApi</display-name>
  	<description></description>
  	<servlet-class>bfsl.FetchApi</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FetchNews</servlet-name>
  	<display-name>FetchNews</display-name>
  	<description></description>
  	<servlet-class>bfsl.FetchNews</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FetchApi</servlet-name>
  	<url-pattern>/FetchApi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FetchNews</servlet-name>
  	<url-pattern>/FetchNews</url-pattern>
  </servlet-mapping>
</web-app>
